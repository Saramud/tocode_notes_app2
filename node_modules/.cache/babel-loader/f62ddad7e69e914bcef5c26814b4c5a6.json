,OAXA;AAXA,KAAA;AA6BA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AAAA,uBACA,KAAA,IADA;AAAA,UACA,KADA,cACA,KADA;AAAA,UACA,KADA,cACA,KADA;;AAEA,UAAA,KAAA,KAAA,EAAA,EAAA;AACA,aAAA,OAAA,GAAA,sBAAA;AACA,eAAA,KAAA;AACA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,KAAA,EAAA,KAFA;AAGA,QAAA,IAAA,EAAA,IAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,EAAA,cAAA,EAHA;AAIA,QAAA,KAAA,EAAA,KAAA;AAJA,OAAA;AAMA,WAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,KAhBA;AAkBA,IAAA,UAlBA,sBAkBA,KAlBA,EAkBA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AApBA,GAtCA;AA4DA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA;AAAA,UAAA,MAAA,GAAA,KAAA,MAAA;AACA,UAAA,CAAA,MAAA,EAAA,OAAA,KAAA;AACA,MAAA,MAAA,GAAA,MAAA,CAAA,IAAA,GAAA,WAAA,EAAA,CAHA,CAGA;;AACA,MAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,OAAA,CAAA,MAAA,MAAA,CAAA,CAAA,EAAA;AACA,iBAAA,IAAA;AACA;AACA,OAJA,CAAA;AAKA,aAAA,KAAA;AACA;AAXA;AA5DA,CAAA","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"wrapper-content\"> \n        <section>\n          <div class=\"container\">\n            \n            <message :msg = 'message'  v-if='message'/>\n            {{message}}\n              <!-- new note -->\n            <newNote :note='note'\n            @addnote='addNote'\n            @priority='prority = $event'\n            :standart = 'standart'\n            />\n            \n            <div class=\"note-header\">\n              <h1>{{ title }}</h1>\n            <!-- Filter -->\n            <search     \n                placeholder=\"find your note\" \n                @poisk=\"search = $event\" \n            /> <!--$event в нашем случае это передаваемое из search.vue val-->\n\n\n            <div class=\"icons\">\n              <svg  v-bind:class=\"{ active : grid }\" @click = \"grid = true\" xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" ><rect x=\"3\" y=\"3\" width=\"7\" height=\"7\"></rect><rect x=\"14\" y=\"3\" width=\"7\" height=\"7\"></rect><rect x=\"14\" y=\"14\" width=\"7\" height=\"7\"></rect><rect x=\"3\" y=\"14\" width=\"7\" height=\"7\"></rect></svg>\n              <svg  v-bind:class=\"{ active : !grid }\" @click = \"grid = false\" xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"><line x1=\"8\" y1=\"6\" x2=\"21\" y2=\"6\"></line><line x1=\"8\" y1=\"12\" x2=\"21\" y2=\"12\"></line><line x1=\"8\" y1=\"18\" x2=\"21\" y2=\"18\"></line><line x1=\"3\" y1=\"6\" x2=\"3\" y2=\"6\"></line><line x1=\"3\" y1=\"12\" x2=\"3\" y2=\"12\"></line><line x1=\"3\" y1=\"18\" x2=\"3\" y2=\"18\"></line></svg>\n            </div>\n            </div>\n              <!-- note list-->\n            <notes :notes='notesFilter' :grid='grid' @remove='removeNote' /> \n          </div>\n        </section>\n      </div>\n    </div>\n\n</template>\n\n<script>\nimport message from '@/components/Message.vue'\nimport newNote from '@/components/NewNote.vue'\nimport notes from '@/components/Notes.vue'\nimport search from '@/components/Search.vue'\n\nexport default {\n  components: {\n    message, \n    newNote,\n    notes,\n    search\n  },\n  data() {\n    return {\n            title: 'Notes App',\n            search: '',\n            grid: true,\n            priority: '',\n            standart: 'white',\n            message: undefined,\n            note: {\n                title: '',\n                descr: '',\n            },\n            notes: [\n                {\n                title: 'First Note',\n                descr: 'Description for first note',\n                date: new Date(Date.now()).toLocaleString()\n            },\n            {\n                title: 'Second Note',\n                descr: 'Description fo second note',\n                date: new Date(Date.now()).toLocaleString()\n            },\n            {\n                title: 'Third Note',\n                descr: 'Description fo third note',\n                date: new Date(Date.now()).toLocaleString()\n            }\n        ]\n        }\n  },\n  methods: {\n            addNote() {\n                let {title, descr} = this.note;\n                if (title === '') {\n                    this.message = \"title can't be blank\";\n                    return false;\n                }\n                this.notes.push({\n                    title,\n                    descr,\n                    date: new Date(Date.now()).toLocaleString(),\n                    color: this.priority,\n                });\n                this.note.title ='';\n                this.note.descr = '';\n                this.message = null;\n            },\n            \n            removeNote(index){\n              this.notes.splice(index,1);\n            },\n        },\n  computed: {\n    notesFilter () {\n      let array = this.notes, search = this.search;\n      if (!search) return array;\n      search = search.trim().toLowerCase(); //метод trim удаляет пробелы в начале и конце\n      array = array.filter(function(item){\n        if (item.title.toLowerCase().indexOf(search) !== -1){\n          return item;\n        }\n      })\n      return array;\n  }\n}\n}\n\n</script>"],"sourceRoot":"src"}]}